{
  "name": "esy-cross-compiler",
  "version": "0.0.0",
  "description": "A wrapper package for a cross-compiler",
  "license": "MIT",
  "esy": {
    "build": [
      [
        "bash",
        "-c",
        "./install.sh"
      ]
    ],
    "buildsInSource": true,
    "exportedEnv": {
      "ESY_CROSS_COMPILER_TARGET": {
        "val": "x86_64-linux-android",
        "scope": "global"
      },
      "ESY_CROSS_COMPILER_HOST": {
        "val": "x86_64-linux-android24",
        "scope": "global"
      },
      "ESY_CROSS_COMPILER_CC": {
        "val": "x86_64-linux-android24-clang #{esy-cross-compiler.lib / 'sys_shm.o'}",
        "scope": "global"
      }
    }
  }
}
